<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.triviaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TriviaApp">
        <activity android:name=".history.ActivityHistory"></activity>
        <activity android:name=".summary.Summary" />
        <activity android:name=".secondQuestion.SecondQuestion" />
        <activity android:name=".quizPage.QuizPage" />
        <activity android:name=".firstPage.FirstPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>